Mods: {
	VacuumBags: {
		Config: {
			BagEffectOptions: {
				BannerBagNumberOfBannersInInventory: {
					Label: Sac de bannières Nombre de bannières en inventaire
					Tooltip:
						'''
						Lorsque le sac de bannières est dans votre inventaire, le premier nombre X de bannières appliquera leurs buffs.
						Favoriser une ou plusieurs bannières leur donne la priorité sur celles qui ne sont pas favorites.
						Réglez sur -1 pour que toutes les bannières du sac donnent leurs buffs.
						'''
				}

				BannerBagNumberOfBannersWhenPlaced: {
					Label: Nombre de bannières dans le sac une fois placées
					Tooltip:
						'''
						Lorsque le sac de bannières est placé dans le monde, le premier nombre X de bannières appliquera leurs buffs.
						Favoriser une ou plusieurs bannières leur donne la priorité sur celles qui ne sont pas favorites. Réglez à -1 pour toutes les bannières du sac pour donner leurs buffs.
						'''
				}

				PortableStationNumberOfStationsInInventory: {
					Label: Station portative Nombre de stations en inventaire
					Tooltip:
						'''
						Lorsque la station portable est dans votre inventaire, le premier nombre X de stations sera disponible pour la fabrication.
						Favoriser une ou plusieurs stations leur donne la priorité sur celles qui ne sont pas favorites.
						Réglez sur -1 pour que toutes les stations de la station portable soient disponibles pour la fabrication.
						'''
				}

				PortableStationNumberOfStationsWhenPlaced: {
					Label: Station portative Nombre de stations une fois placées
					Tooltip:
						'''
						Lorsque la station portable est placée dans le monde, le premier nombre X de stations sera disponible pour la fabrication.
						Favoriser une ou plusieurs stations leur donne la priorité sur celles qui ne sont pas favorites.
						Réglez sur -1 pour que toutes les stations de la station portable soient disponibles pour la fabrication.
						'''
				}
			}

			BagStorageOptions: {
				AllAmmoItemsGoIntoAmmoBag: {
					Label: Toutes les munitions vont dans le sac de munitions
					Tooltip:
						'''
						Si cela est vrai, tous les objets utilisés comme munitions pour quoi que ce soit iront dans le sac.
						Si faux, seules les munitions sélectionnées sur la liste blanche seront autorisées, ce qui exclut quelques éléments comme les étoiles.
						'''
				}

				SimpleBagStorageSize: {
					Label: Taille de stockage du sac simple
					Tooltip:
						'''
						Le nombre d'emplacements dans le rangement simple du sac.
						Si cette option est utilisée alors que le joueur a plus d'objets dans son sac, la taille du sac sera réduite au nombre d'objets encore dans le sac.
						Définir ce nombre très élevé pourrait entraîner un décalage ou éventuellement un crash dû à une utilisation trop importante de mémoire si vous n'avez pas beaucoup de RAM. À utiliser à vos risques et périls.
						'''
				}

				SimpleBagsVacuumAllItems: {
					Label: Sacs simples pour aspirer tous les articles
					Tooltip:
						'''
						Si cela est vrai, tous les articles seront aspirés dans de simples sacs.
						Si faux, seuls les articles du même type qu’un article dans le sac seront aspirés.
						'''
				}
			}

